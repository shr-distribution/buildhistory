\tcd $D/etc/init.d\n\n\techo "Disabling the following sysv scripts: "\n\n\tOPTS=""\n\n\tif [ -n "$D" ]; then\n\t\tOPTS="--root=$D"\n\tfi\n\n\tfor i in    busybox-udhcpc   dnsmasq   hwclock   syslog   syslog.busybox   shr-splash   keymap   fbsetup   populate-volatile   mountall   bootmisc   mountnfs   checkroot   modutils   fso-gpsd-sysv   gpsd-sysv  ; do\n\t\tif [ \\( -e $i -o $i.sh \\) -a ! \\( -e $D/etc/systemd/system/$i.service -o  -e $D/lib/systemd/system/$i.service \\) ] ; then\n\t\t\techo -n "$i: " ; systemctl ${OPTS} mask $i.service\n\t\tfi\n\tdone ; echo\n