cd $D/etc/init.d

echo -n "Disabling the following sysv scripts: "

for i in    busybox-udhcpc   dnsmasq   hwclock   syslog   syslog.busybox   shr-splash   crond   keymap   fbsetup   populate-volatile   mountall   bootmisc   mountnfs   checkroot   modutils   fso-gpsd-sysv   gpsd-sysv  ; do
    if [ \( -e $i -o $i.sh \) -a ! -e $D/lib/systemd/system/$i.service ] ; then
        systemctl mask $i.service
    fi
done ; echo
